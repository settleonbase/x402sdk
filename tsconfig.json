{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "outDir": "./build",
    "rootDir": "./src",
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2022"],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/localServer/num.js",
    "src/localServer/workers/sw.ts.old"
  ]
}
